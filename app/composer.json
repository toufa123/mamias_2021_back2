{
    "name": "mamias/2020",
    "type": "project",
    "license": "proprietary",
    "description": "MAMIAS webApp 3.0 ",
    "require": {
        "php": "^7.3|^8.0",
        "ext-ctype": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "api-platform/api-pack": "^1.3.0",
        "cocur/slugify": "^4.0",
        "creof/doctrine2-spatial": "dev-master",
        "creof/geo-parser": "^2.2.1",
        "creof/wkt-parser": "^2.2",
        "doctrine/doctrine-migrations-bundle": "3.0.1",
        "ersah/ga-bundle": "dev-master",
        "excelwebzone/recaptcha-bundle": "^1.5",
        "guzzlehttp/guzzle": "^6.5.5",
        "helios-ag/fm-elfinder-bundle": "^10.0.4",
        "jms/serializer-bundle": "^3.7.0",
        "jsor/doctrine-postgis": "^1.7",
        "liip/imagine-bundle": "^2.4",
        "mhujer/breadcrumbs-bundle": "1.5.0",
        "ob/highcharts-bundle": "^1.7",
        "orbitale/cms-bundle": "^3.1.1",
        "phpoffice/phpspreadsheet": "^1.16.0",
        "picoss/sonata-extra-admin-bundle": "^4.0",
        "presta/sitemap-bundle": "^2.3",
        "pugx/autocompleter-bundle": "^1.7",
        "redcode/tree-bundle": "^0.1.0@alpha",
        "rollerworks/password-strength-bundle": "2.2.0",
        "sensio/framework-extra-bundle": "^5.6.1",
        "sensiolabs/security-checker": "^6.0.3",
        "sonata-project/admin-bundle": "^3.88.0",
        "sonata-project/block-bundle": "^3.22.0",
        "sonata-project/cache-bundle": "^2.4.2",
        "sonata-project/classification-bundle": "*",
        "sonata-project/doctrine-orm-admin-bundle": "3.21.*",
        "sonata-project/formatter-bundle": "4.3.0",
        "sonata-project/media-bundle": "^3.28.0",
        "sonata-project/news-bundle": "3.15.0",
        "sonata-project/translation-bundle": "2.4.*",
        "sonata-project/user-bundle": "4.5.*",
        "sonata-project/doctrine-extensions": "^1.10.1",
        "stof/doctrine-extensions-bundle": "*",
        "swiftmailer/swiftmailer": "^6.2.5",
        "symfony/asset": "^4.4|^5.0.0",
        "symfony/console": "4.4.18",
        "symfony/expression-language": "4.4.18",
        "symfony/flex": "^1.9.1",
        "symfony/form": "4.4.18",
        "symfony/framework-bundle": "4.4.18",
        "symfony/http-client": "4.4.18",
        "symfony/intl": "4.4.18",
        "symfony/mailer": "4.4.18",
        "symfony/monolog-bundle": "^3.6",
        "doctrine/doctrine-bundle": "^1.8",
        "symfony/process": "4.4.18",
        "symfony/security-bundle": "4.4.18",
        "symfony/serializer-pack": "1.0.4",
        "symfony/swiftmailer-bundle": "3.4.0",
        "symfony/translation": "4.4.18",
        "symfony/twig-bundle": "4.4.18",
        "symfony/validator": "4.4.18",
        "symfony/web-link": "4.4.18",
        "symfony/yaml": "4.4.18",
        "symfonycasts/reset-password-bundle": "*",
        "twig/twig": "^2.12.5",
        "vich/uploader-bundle": "1.11.*",
        "whiteoctober/breadcrumbs-bundle": "^1.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17.3",
        "phpmd/phpmd": "^2.9.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^0.12.65",
        "squizlabs/php_codesniffer": "^3.5.8",
        "symfony/debug-pack": "1.0.9",
        "symfony/dotenv": "4.4.*",
        "symfony/maker-bundle": "^1.26.1",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "symfony/var-dumper": "4.4.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {

        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "elfinder:install": "symfony-cmd",
            "doctrine:schema:update --force": "symfony-cmd",
            "security:check": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "php bin/console doctrine:schema:update --force",
            "php bin/console ckeditor:install",
            "php bin/console elfinder:install",
            "php bin/console security:check"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "php bin/console doctrine:schema:update --force",
            "php bin/console ckeditor:install",
            "php bin/console elfinder:install",
            "php bin/console security:check"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "require": "4.4.18",
            "public-dir": "/public/"
        },
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
